[gd_scene load_steps=5 format=2]

[ext_resource path="res://client/client.gd" type="Script" id=1]
[ext_resource path="res://world/world.tscn" type="PackedScene" id=2]
[ext_resource path="res://client/client_control.gd" type="Script" id=3]
[ext_resource path="res://client/gui/client_gui.tscn" type="PackedScene" id=4]

[node name="game" type="Node"]
script = ExtResource( 1 )

[node name="network_tick" type="Timer" parent="."]
process_mode = 1
wait_time = 0.1
one_shot = false
autostart = false

[node name="world" parent="." instance=ExtResource( 2 )]

[node name="players" type="Node" parent="world"]

[node name="client_control" type="Node2D" parent="."]
script = ExtResource( 3 )
_sections_unfolded = [ "Script" ]

[node name="gui" parent="." instance=ExtResource( 4 )]

[connection signal="timeout" from="network_tick" to="." method="_on_network_tick"]
